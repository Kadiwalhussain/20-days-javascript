    <script>
        function updateCountdown() {
            const now = new Date();
            const firstExam = new Date('2025-11-12T14:30:00');
            const diff = firstExam - now;

            if (diff > 0) {
                const days = Math.floor(diff / (1000 * 60 * 60 * 24));
                const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
                
                document.getElementById('countdown').innerHTML = 
                    `‚è∞ First exam starts in: <strong>${days}</strong> days, <strong>${hours}</strong> hours, <strong>${minutes}</strong> minutes`;
            } else {
                document.getElementById('countdown').innerHTML = 'üéØ Exam period is ongoing or completed!';
            }
        }

        updateCountdown();
        setInterval(updateCountdown, 60000);
    </script>
